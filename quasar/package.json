{
  "name": "EFTG",
  "version": "0.1.0",
  "description": "",
  "productName": "EFTG",
  "cordovaId": "org.cordova.quasar.app",
  "author": "Kevin Marrec",
  "private": true,
  "scripts": {
    "lint": "eslint {src,test}/**/*.{js,vue}",
    "lint:fix": "eslint --fix {src,test}/**/*.{js,vue}",
    "dev": "NODE_OPTIONS=--max_old_space_size=4096 DEBUG=v8:* quasar dev",
    "dev:ssr": "NODE_OPTIONS=--max_old_space_size=4096 DEBUG=v8:* quasar dev -m ssr",
    "test:unit": "jest",
    "test:cover": "jest --coverage",
    "test:cover:open": "quasar serve test/coverage/lcov-report/ --cache 0 --port 8788 --open",
    "build": "quasar build",
    "build:ssr": "quasar build -m ssr",
    "build:clean": "quasar clean",
    "serve": "quasar serve ./dist/spa-mat",
    "serve:ssr": "quasar serve ./dist/ssr-mat",
    "deploy": "now dist/spa-mat",
    "deploy:ssr": "now dist/ssr-mat"
  },
  "dependencies": {
    "@truthy/truth-runner": "^0.3.5",
    "fuse.js": "^3.2.1",
    "vue-i18n": "^8.1.0",
    "vue-pdf": "^3.3.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "codecov": "^3.1.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "jest": "^23.5.0",
    "jest-serializer-vue": "^2.0.2",
    "now": "^11.4.5",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "quasar-cli": "^0.17.19",
    "strip-ansi": "=3.0.1",
    "vue-jest": "^2.6.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
